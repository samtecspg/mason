    "id": { "type": ["string", "integer"]},
    "metastore_engine": {
      "type": "string",
      "enum": [
        "glue",
        "s3",
        "athena"
      ]
    },
    "execution_engine": {
      "type": "string",
      "enum": [
        "spark",
        "athena",
        "dask",
        "local"
      ]
    },
    "storage_engine": {
      "type": "string",
      "enum": [
        "s3"
      ]
    },
    "scheduler_engine": {
      "type": "string",
      "enum": [
        "glue",
        "local",
        "airflow"
      ]
    },
    "clients": {
      "type": "object",
      "properties": {}
    }
  },
  "additionalProperties": false,
  "required": [ "id" ]
}

